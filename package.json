{
  "homepage": "https://diminishedprime.github.io/metronome",
  "name": "metronome",
  "version": "20.0.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^1.0.0",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-saga": "^0.15.4"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-redux-saga": "^0.4.0",
    "prettier": "^1.12.1",
    "react-scripts": "1.0.10"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "predeploy": "npm version major && npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --write 'src/**/*.jsx' 'src/**/*.js' package.json .eslintrc.js",
    "lint": "eslint . --ignore-path .eslintignore --ext .js --ext .jsx"
  }
}
